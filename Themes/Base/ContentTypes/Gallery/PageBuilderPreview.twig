<div class="content-type-edit-gallery-wrapper">
    <div class="gallery {{ data.css }}">
        {% for dataImage in data.images|default({}) %}
            <img class="gallery__item" src="{{ media(dataImage, 200) }}" alt="{{ dataImage }}" width="200"/>
        {% else %}
            {{ i18n('content_type_gallery_no_images', {en: 'No images yet', de: 'Noch keine Bilder'}) }}
        {% endfor %}
    </div>
</div>

