{% if value is not empty %}
    {% set val = getInsertionValue(value) %}
    <span class="main--value">{{ i18n(val.value) }}</span>
    {% for subAttribute in attribute.values %}
        {% if subAttribute.subAttributeKey != null and subAttribute.id == val.id %}
            {% if insertion_values %}
                {% include 'Plugins/Insertion/Frontend/FrontendInsertion/DetailAttributes/Values/' ~ all_attributes[subAttribute.subAttributeKey].type|capitalize ~'.twig'  with {'attribute': all_attributes[subAttribute.subAttributeKey], 'value': insertion_values[subAttribute.subAttributeKey] } %}
            {% elseif item.insertion_values %}
                {{ o_dump(all_attributes[subAttribute.subAttributeKey]) }}
                {% include 'Plugins/Insertion/Frontend/FrontendInsertion/DetailAttributes/Values/' ~ all_attributes[subAttribute.subAttributeKey].type|capitalize ~'.twig'  with {'attribute': all_attributes[subAttribute.subAttributeKey], 'value': item.insertion_values[subAttribute.subAttributeKey] } %}
            {% endif %}
        {% endif %}
    {% endfor %}
{% endif %}
