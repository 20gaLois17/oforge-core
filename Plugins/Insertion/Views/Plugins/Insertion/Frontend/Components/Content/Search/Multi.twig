{#
<div class="form__control  form__control--multi">
<div class="multi-select"
data-sub-attribute-select="true"
data-multi-select="{{ attribute.name }}[]"
{% if required %}data-required{% endif %}>
<p class="multi-select__search"
data-multi-select-search
data-placeholder="{{ i18n(attribute.name) }}"></p>
<ul class="multi-select__list"
data-multi-select-list>
{% for value in attribute.values %}
<li class="multi-select__item {% if value.id in search.filter[attribute.name] %}multi-select__item--checked{% endif %}"
data-multi-select-item="{{ value.id }}">
{{ i18n(value.value) }}
</li>
{% endfor %}
</ul>
</div>
</div>
#}

<div class="form__control form__control--new-select form__control--multi">

    <div class="form__control form__control--new-select">
        <div class="select"
             data-select-type="multi"
             data-placeholder="choose stuff">
            <p class="select__text"
               data-select-text=""></p>
            <ul class="select__list"
                data-select-list="">
                <li class="select__item"
                    data-selected="false"
                    data-index="0"
                    data-value-id="0">
                    <span class="select__value">
                    Val0
                    </span>
                    <div class="container">
                        <div class="select select--is-sub"
                             data-select-type="single"
                             data-placeholder="select sub"
                             data-sub-select="">
